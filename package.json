{
    "name": "my-app",
    "version": "0.0.1",
    "license": "OSL-3.0",
    "dependencies": {
        "@scandipwa/scandipwa": "5.2.4",
        "@scandipwa/scandipwa-scripts": "2.4.19",
        "@scandipwa/eslint-config": "0.1.36",
        "@tilework/mosaic": "0.1.4",
        "@tilework/mosaic-craco": "0.0.5",
        "google-maps-react": "^2.0.6",
        "page-builder": "file:packages/page-builder",
        "postcss": "8.4.5",
        "scandipwa-cli": "^0.3.15"
    },
    "scandipwa": {
        "type": "theme",
        "locales": {
            "en_US": true
        },
        "parentTheme": "@scandipwa/scandipwa",
        "extensions": {
            "packages/page-builder": true
        }
    },
    "scripts": {
        "start": "scandipwa-scripts start",
        "build": "scandipwa-scripts build",
        "postinstall": "scandipwa-scripts link",
        "postupdate": "scandipwa-scripts link"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": "@scandipwa"
    },
    "proxy": "https://magento.aayke.com/"
}
